{
  "name": "phonegap-app-showcase",
  "version": "0.0.1",
  "description": "The app showcase found at http://phonegap.github.io/app",
  "main": "index.js",
  "scripts": {
    "build": "node build_data.js",
    "deploy": "git subtree push --prefix _site origin gh-pages",
    "clean": "rm -rf _site/*",
    "watch": "watch 'npm run build' ./src",
    "browser-sync": "browser-sync start --server _site --files _site",
    "start": "npm run watch & npm run browser-sync",
    "test": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/phonegap/app"
  },
  "keywords": [
    "phonegap"
  ],
  "author": "Garth Braithwaite <garthdb@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/phonegap/app/issues"
  },
  "homepage": "http://phonegap.github.io/app/",
  "devDependencies": {
    "autoprefixer": "^6.1.2",
    "browser-sync": "^2.11.1",
    "escape-html": "^1.0.3",
    "jade": "^1.11.0",
    "mkdirp": "^0.5.1",
    "yamljs": "^0.2.4"
  }
}
